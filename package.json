    "@nextui-org/react": "^2.1.13",
    "@prisma/client": "^5.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@upstash/redis": "^1.23.3",
    "axios": "^1.5.1",
    "bad-words": "^3.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.286.0",
    "nanoid": "^5.0.1",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "next-themes": "^0.2.1",
    "openai": "^4.12.1",
    "pusher": "^5.1.3",
    "pusher-js": "^8.3.0",
    "raw-body": "^2.5.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-loading-skeleton": "^3.3.1",
    "react-textarea-autosize": "^8.5.3",
    "stripe": "^13.10.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4"
  },
